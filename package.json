    "axios": "^0.26.0",
    "formik": "^2.2.9",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.16",
    "react-redux": "^7.2.6",
    "redux-logger": "^3.0.6",
    "rollbar": "^2.24.0"
